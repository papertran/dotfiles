{
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "Catppuccin Mocha",
  "window.zoomLevel": 0,
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Menlo, Monaco, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.45,
  "editor.letterSpacing": 0.2,
  "editor.renderWhitespace": "selection",
  "editor.renderControlCharacters": false,
  "editor.minimap.enabled": false,
  "editor.glyphMargin": false,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.stickyScroll.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.editor.enablePreview": true,
  "workbench.statusBar.visible": true,
  "editor.hover.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "problems.showCurrentInStatus": true,
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.idea": true,
    "**/.vscode": false,
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.git": true
  },
  "explorer.compactFolders": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": false,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "prettier.semi": true,
  "prettier.arrowParens": "always",
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'Fira Code', Menlo, monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.defaultLocation": "view",
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false
  },
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "typescript.format.semicolons": "insert",
  "json.format.keepLines": false,
  "python.analysis.typeCheckingMode": "basic",
  "python.formatting.provider": "black",
  "editor.wordWrap": "on",
  "editor.smoothScrolling": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "editor.suggestSelection": "first",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.snippetSuggestions": "inline",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.linkedEditing": true,
  "editor.suggest.showWords": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.labelFormat": "medium",
  "window.restoreWindows": "all",
  "telemetry.telemetryLevel": "off",
  "update.showReleaseNotes": false,
  "extensions.autoUpdate": true,
  "extensions.autoCheckUpdates": true,
  "eslint.alwaysShowStatus": false,
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.rules": null,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "vim.leader": "<space>",
  "vscode-neovim.ctrlKeysForInsertMode": [
    "a",
    "d",
    "h",
    "j",
    "o",
    "r",
    "t",
    "u",
    "w"
  ],
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "b",
    "d",
    "e",
    "f",
    "h",
    "i",
    "j",
    "k",
    "l",
    "m",
    "o",
    "r",
    "t",
    "u",
    "v",
    "w",
    "x",
    "y",
    "z",
    "/"
  ],
  "explorer.confirmDragAndDrop": false,
  "cursor.cpp.disabledLanguages": ["plaintext"],
  "terminal.integrated.defaultProfile.osx": "fish",
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3.13"
}
